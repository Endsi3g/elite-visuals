{
  "name": "elite-visuals",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "deploy": "vercel --prod",
    "deploy:preview": "vercel",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest --config jest.config.js",
    "test:simple": "jest --config jest.config.simple.js",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:dashboard": "node scripts/test-dashboard.js",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:all": "npm run test && npm run test:e2e",
    "test:autonoma": "ts-node examples/autonoma-test.ts",
    "audit:accessibility": "ts-node scripts/accessibility-audit.ts",
    "audit:accessibility:fix": "ts-node scripts/accessibility-audit.ts --fix",
    "test:accessibility": "playwright test e2e/accessibility.spec.ts",
    "test:performance": "playwright test e2e/performance.spec.ts",
    "test:landing": "playwright test e2e/landing.spec.ts",
    "test:ai-tasks": "playwright test e2e/ai-tasks.spec.ts",
    "lighthouse": "lighthouse http://localhost:3000 --output=html --output-path=./reports/lighthouse.html --preset=desktop",
    "lighthouse:ci": "powershell -ExecutionPolicy Bypass -File scripts/lighthouse-ci.ps1",
    "test:all-local": "powershell -ExecutionPolicy Bypass -File scripts/run-all-tests.ps1",
    "audit:project": "npx ts-node scripts/project-audit-agent.ts",
    "audit:connections": "npx ts-node scripts/validate-connections.ts",
    "audit:full": "npx ts-node scripts/run-full-audit.ts",
    "audit:full:commit": "npx ts-node scripts/run-full-audit.ts --auto-commit",
    "audit:security": "powershell -ExecutionPolicy Bypass -File scripts/security-check.ps1",
    "security:check": "npm audit",
    "security:fix": "npm audit fix --legacy-peer-deps"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.25.2",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@radix-ui/react-dialog": "^1.1.1",
    "@radix-ui/react-dropdown-menu": "^2.1.1",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-tabs": "^1.1.0",
    "@radix-ui/react-toast": "^1.2.1",
    "@supabase/auth-helpers-nextjs": "^0.10.0",
    "@supabase/supabase-js": "^2.39.0",
    "@types/node": "^20.14.15",
    "@types/react": "^18.3.3",
    "@types/react-beautiful-dnd": "^13.1.8",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.20",
    "axios": "^1.7.9",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "framer-motion": "^11.3.28",
    "jspdf": "^3.0.4",
    "konva": "^9.3.14",
    "langchain": "^0.3.5",
    "lucide-react": "^0.427.0",
    "next": "^15.0.3",
    "node-fetch": "^2.7.0",
    "postcss": "^8.4.41",
    "react": "^18.3.1",
    "react-beautiful-dnd": "^13.1.1",
    "react-dom": "^18.3.1",
    "react-dropzone": "^14.2.3",
    "react-konva": "^18.2.10",
    "react-markdown": "^9.0.1",
    "redis": "^4.6.0",
    "tailwind-merge": "^2.5.2",
    "tailwindcss": "^3.4.9",
    "tailwindcss-animate": "^1.0.7",
    "tesseract.js": "^5.0.0",
    "typescript": "^5.5.4",
    "y-websocket": "^1.5.0",
    "yjs": "^13.6.10",
    "zustand": "^4.5.4"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.8.2",
    "@axe-core/react": "^4.8.2",
    "@playwright/test": "^1.40.0",
    "@swc/jest": "^0.2.39",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/jest": "^29.5.11",
    "@use-gesture/react": "^10.3.0",
    "eslint": "^9.39.1",
    "eslint-config-next": "^15.5.6",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lighthouse": "^12.8.2",
    "ts-node": "^10.9.2"
  }
}
