# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=your_supabase_project_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key

# Ollama (LLM Open Source - Local)
OLLAMA_BASE_URL=http://localhost:11434
OLLAMA_MODEL=llama3

# HuggingFace (Fallback & Whisper)
HUGGINGFACE_API_KEY=hf_your_key

# Anthropic Claude (Optional)
ANTHROPIC_API_KEY=sk-ant-your_anthropic_key

# Luma AI (Optional)
LUMA_API_KEY=your_luma_key
LUMA_API_URL=https://api.lumalabs.ai/v1

# Autonoma Testing (Optional)
AUTONOMA_CLIENT_ID=your_autonoma_client_id
AUTONOMA_CLIENT_SECRET=your_autonoma_client_secret

# Figma Integration (Optional)
FIGMA_ACCESS_TOKEN=your_figma_access_token

# Redis Cache (Optional - Production)
REDIS_URL=redis://localhost:6379

# Rate Limiting
RATE_LIMIT_MAX_REQUESTS=10
RATE_LIMIT_WINDOW_MINUTES=1
